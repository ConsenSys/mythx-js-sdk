  "repository": {
    "type": "git",
    "url": "git+https://github.com/ConsenSys/mythx-js-sdk.git"
  },
  "author": "Daniyar Chambylov <dan.chambylov@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ConsenSys/mythx-js-sdk/issues"
  },
  "homepage": "https://github.com/ConsenSys/mythx-js-sdk#readme",
  "devDependencies": {
    "@types/assert": "^1.4.1",
    "@types/async": "^2.0.50",
    "@types/debug": "^4.1.2",
    "@types/find-cache-dir": "^2.0.0",
    "@types/fs-extra": "^5.0.5",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.18",
    "@types/proxyquire": "^1.3.28",
    "@types/request-promise": "^4.1.42",
    "@types/semver": "^5.5.0",
    "@types/sinon": "^7.0.6",
    "assert": "^1.4.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.9",
    "nyc": "^13.1.0",
    "proxyquire": "^2.1.0",
    "sinon": "^7.2.4",
    "source-map-support": "^0.5.10",
    "ts-mocha": "^2.0.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "typescript": "^3.2.2"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "dist"
    ],
    "all": true
  },
  "dependencies": {
    "armlet": "^0.3.1",
    "colors": "^1.3.3",

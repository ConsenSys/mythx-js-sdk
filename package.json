    "require-from-string": "^2.0.2",
    "semver": "^5.6.0",
    "solc": "^0.5.6",
    "truffle-artifactor": "^4.0.8",
    "truffle-config": "^1.1.6",
    "truffle-contract-sources": "^0.1.3",
    "truffle-error": "0.0.4",
    "truffle-expect": "0.0.7",
    "truffle-external-compile": "^1.0.5",
    "truffle-resolver": "^5.0.8"
